#include <stdio.h>
int add(int , int);
int main()
{
    int a,b,res;
    printf("Enter num1 : \n");
    scanf("%d",&a);
    printf("Enter num2 : \n");
    scanf("%d",&b);
    res = add(a,b);
    printf("Val %d\n",res);

    return 0;
}
int add(int a,int b)
{
int temp;
    temp = a+b;
    return temp;
}
